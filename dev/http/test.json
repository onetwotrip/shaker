{
  "url": "http://127.0.0.1",
  "jobs": [
    {
      "name": "Every minute",
      "cron": "* * * * *",
      "uri": "api/myMethod1"
    },
    {
      "name": "Every minute 2 with 10 second timeout",
      "cron": "* * * * *",
      "uri": "api/myMethod2",
      "timeout": 10
    },
    {
      "name": "Every 4 minutes",
      "cron": "*/4 * * * *",
      "uri": "api/myMethod3"
    },
    {
      "name": "Every day at 1 hour 0 minutes with user user1, method post, content-type header",
      "cron": "0 1 * * *",
      "uri": "api/myMethod4",
      "user": "user1",
      "method": "post",
      "body": {},
      "contentType": "application/json",
      "userAgent": "test-user"
    }
  ]
}
